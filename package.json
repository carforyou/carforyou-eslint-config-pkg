{
  "name": "@carforyou/eslint-config",
  "version": "0.0.0",
  "description": "ESLint rules that we use at CAR FOR YOU",
  "scripts": {
    "version": "npm run build",
    "build": "pika build",
    "lint": "eslint --ext ts,js,tsx,jsx,json .",
    "format": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "test:unit": "jest",
    "test:debug": "node --inspect-brk --inspect=127.0.0.1:9229 ./node_modules/jest/bin/jest.js --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carforyou/carforyou-eslint-rules-pkg.git"
  },
  "author": "CAR FOR YOU",
  "license": "MIT",
  "homepage": "https://github.com/carforyou/carforyou-eslint-rules-pkg#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.11.0",
    "eslint-plugin-jsdoc": "^25.4.1",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-testing-library": "^3.1.2",
    "prettier": "^2.0.5",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-copy-assets": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@types/jest": "^25.2.2",
    "jest": "^25.5.4",
    "semantic-release": "^17.0.7",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "./packMain.js"
      ],
      [
        "@pika/plugin-copy-assets",
        {
          "files": [
            "index.js",
            "react.js",
            "next.js",
            "shared.js"
          ]
        }
      ]
    ]
  }
}
